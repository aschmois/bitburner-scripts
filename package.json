{
  "name": "bitburner-scripts",
  "version": "1.0.0",
  "description": "scripts for https://github.com/danielyxie/bitburner",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.4.3",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "@types/react": "^17.0.21",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.13",
    "@types/react-resizable": "^1.7.3",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.7"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "defs": "node ./updateDefs.js",
    "clean": "rimraf out",
    "prebuild": "npm run lint",
    "build": "npm run defs && tsc -p tsconfig.json",
    "build:watch": "npm run defs && tsc -w -p tsconfig.json",
    "prettier": "prettier --config .prettierrc --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aschmois/bitburner-scripts.git"
  },
  "author": "aschmois",
